# ObsidianEcho-AI Environment Variables
# Copy this file to .env and fill in your values

# Application Settings
OEA_APP_NAME=ObsidianEcho-AI
OEA_VERSION=0.1.0
OEA_DEBUG=false

# Server Settings
OEA_HOST=0.0.0.0
OEA_PORT=8000

# Logging
OEA_LOG_LEVEL=INFO
OEA_LOG_FORMAT=json

# CORS Settings (comma-separated)
OEA_CORS_ORIGINS=*

# AI Provider API Keys (to be added in future stories)
# OEA_OPENAI_API_KEY=your-openai-api-key-here
# OEA_XAI_API_KEY=your-xai-api-key-here

# Rate Limiting (to be added in future stories)
# OEA_RATE_LIMIT_REQUESTS_PER_MINUTE=10
# OEA_RATE_LIMIT_REQUESTS_PER_HOUR=100

# Task Queue (to be added in future stories)
# OEA_TASK_QUEUE_MAX_WORKERS=4
# OEA_TASK_QUEUE_TIMEOUT_SECONDS=600
